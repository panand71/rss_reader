<h1>CNN Headlines</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @entries.each do |entry| %>
    <tr>

      <td><%= link_to entry.title, entry.url, target: "_blank" %></td>
      <td><%= link_to 'View Story', entry %></td>
    </tr>
    <% end %>   
  </tbody>
</table>

<br>
